    "item": [
        {
            "name": "Health",
            "item": [
                {
                    "name": "Health Check",
                    "request": {
                        "method": "GET",
                        "url": "{{baseUrl}}/health"
                    }
                }
            ]
        },
        {
            "name": "Assistants",
            "description": "AI Assistant configuration management",
            "item": [
                {
                    "name": "Create Assistant",
                    "request": {
                        "method": "POST",
                        "url": "{{baseUrl}}/api/assistants",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"name\": \"Sales Agent\",\n  \"instructions\": \"You are a friendly sales assistant. Introduce yourself and ask how you can help.\",\n  \"first_message\": \"Hello! This is Alex from Vobiz. How are you doing today?\",\n  \"voice\": {\n    \"provider\": \"openai\",\n    \"voice_id\": \"alloy\"\n  },\n  \"webhook_url\": \"https://your-webhook.com/events\"\n}"
                        }
                    }
                },
                {
                    "name": "List Assistants",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{baseUrl}}/api/assistants?is_active=true&limit=50&skip=0",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "api",
                                "assistants"
                            ],
                            "query": [
                                {
                                    "key": "is_active",
                                    "value": "true"
                                },
                                {
                                    "key": "limit",
                                    "value": "50"
                                },
                                {
                                    "key": "skip",
                                    "value": "0"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Get Assistant",
                    "request": {
                        "method": "GET",
                        "url": "{{baseUrl}}/api/assistants/{{assistant_id}}"
                    }
                },
                {
                    "name": "Update Assistant",
                    "request": {
                        "method": "PATCH",
                        "url": "{{baseUrl}}/api/assistants/{{assistant_id}}",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"name\": \"Updated Sales Agent\",\n  \"instructions\": \"Updated instructions here\",\n  \"is_active\": true\n}"
                        }
                    }
                },
                {
                    "name": "Delete Assistant",
                    "request": {
                        "method": "DELETE",
                        "url": "{{baseUrl}}/api/assistants/{{assistant_id}}"
                    }
                }
            ]
        },
